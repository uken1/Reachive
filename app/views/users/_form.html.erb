<%= form_for([@users], remote: true) do |f| %>
  <% if @users.errors.any? %>
    <div id="error_explanation">
      <h2><%= @users.errors.count %>件のエラーがあります。</h2>

      <ul>
      <% @users.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <div class="actions">
  <%= link_to '戻る', users_path, class: 'btn btn-default btn-sm btn-success' %>
  </div>
<% end %>